<section class="sidebar">
    {% block sidebar_profile %}
        {% if app.user %}
            {% block sidebar_profile_authenticated %}
                {% include "UserBundle:Sidebar:authenticated.html.twig" %}
            {% endblock sidebar_profile_authenticated %}
        {% else %}
            {% block sidebar_profile_not_authenticated %}
                {% include "UserBundle:Sidebar:not_authenticated.html.twig" %}
            {% endblock sidebar_profile_not_authenticated %}
        {% endif %}
    {% endblock sidebar_profile %}
    {% block sidebar_menu %}
        {{ knp_menu_render('UserBundle:DashboardMenuBuilder:sidebarMenu') }}
    {% endblock sidebar_menu %}
</section>